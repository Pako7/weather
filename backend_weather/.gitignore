# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal
/db/*.sqlite3-shm
/db/*.sqlite3-wal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development and test.
/storage/*
!/storage/.keep

# Ignore dotenv environment variables file.
/.env
/.env.*
/.envs

# Ignore all local precompiled assets.
/public/assets
/public/packs
/public/packs-test
/public/packs-dev
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity
/.byebug_history

# Ignore the 'coverage' directory generated by tools like SimpleCov.
coverage/

# Ignore system-generated files.
/.DS_Store
/.sass-cache/
/.sass-cache/
/.bundle/
/vendor/bundle
/vendor/cache
/vendor/assets/components
/vendor/assets/bower_components
/typings
/.rbenv-vars
/byebug_history

# Ignore all files generated by editors or IDEs.
*.swp
*.swo
*~
.project
.idea/
.secret
.vscode/
*.iml

# Ignore spring files.
/spring/*

# Ignore Rails master key (used for credentials).
/config/master.key
/config/credentials.yml.enc

# Ignore pidfiles, socket files, and lock files.
/tmp/pids/
/tmp/sockets/
/tmp/*.pid
/tmp/*.lock

# Ignore node_modules folder.
/node_modules

# Ignore webpacker config.
/public/packs/manifest.json
/public/assets/manifest.json
/public/packs-test/manifest.json
/public/packs-dev/manifest.json

# Ignore any gems or packages not in source control.
/vendor/cache/*

